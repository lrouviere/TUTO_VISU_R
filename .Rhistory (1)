moy_var <- function(tbl){
nature <- map_chr(tbl,class)
if (any(nature!="numeric")){
warning(paste("Variable(s)",names(tbl)[nature!="numeric"],"non continue(s)"))
}
res <- map(tbl[,nature=="numeric"],\(x) c(moy=mean(x),var=var(x))) |>
as_tibble()
return(res)
}
