imputation <- function(tbl,choix="moyenne"){
if (choix=="moyenne"){
moy <- map(tbl,\(x) mean(x,na.rm = TRUE))
for (i in 1:ncol(tbl)){
tbl[is.na(tbl[,i]),i] <- moy[i]
}
} else {
for (i in 1:ncol(tbl)){
tbl[is.na(tbl[,i]),i] <- med[i]
}
}
return(tbl)
}
